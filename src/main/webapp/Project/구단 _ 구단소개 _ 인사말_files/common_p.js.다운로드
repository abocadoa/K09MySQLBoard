//div_chg(바꿀divNum,최종개수)
function div_chg(n,m){
	for(i=1; i<=m; i++ ){
		var tab = document.getElementById("li_"+i);
		var bx= document.getElementById("sec_"+i);
		if(i==n){
			bx.style.display="block";
			tab.className="on";
		}else{			
			bx.style.display="none";
			tab.className="";
		}
	}
}
