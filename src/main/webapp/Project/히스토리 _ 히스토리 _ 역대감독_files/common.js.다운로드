{
	if(POPWIN)
	{
		alert("로그인이 시간이 만료되었습니다.");
		return false;
	}

	if(!confirm("로그인 후 이용 가능합니다\n\n로그인 페이지로 이동 하시겠습니까? ("+res+")"))
		return false;
	document.footerLoginForm.submit();
}

